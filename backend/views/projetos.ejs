                get projetosFiltrados() {
                    let filtrados = this.projetos;

                    // Filtro por busca
                    if (this.busca.trim() !== '') {
                        const termoBusca = this.busca.toLowerCase();
                        filtrados = filtrados.filter(projeto =>
                            projeto.nome.toLowerCase().includes(termoBusca) ||
                            projeto.descricao.toLowerCase().includes(termoBusca) ||
                            projeto.tecnologias.some(tech => tech.toLowerCase().includes(termoBusca)) ||
                            projeto.developer.nome.toLowerCase().includes(termoBusca)
                        );
                    }

                    // Filtro por categoria
                    if (this.filtroCategoria !== 'todos') {
                        filtrados = filtrados.filter(projeto => projeto.categoria === this.filtroCategoria);
                    }

                    // Filtro por valor
                    if (this.filtroValor !== 'todos') {
                        filtrados = filtrados.filter(projeto => {
                            const valor = projeto.valorNumerico;
                            switch (this.filtroValor) {
                                case 'ate50k':
                                    return valor <= 50000;
                                case '50k-200k':
                                    return valor > 50000 && valor <= 200000;
                                case 'acima200k':
                                    return valor > 200000;
                                default:
                                    return true;
                            }
                        });
                    }

                    return filtrados;
                }
            }
        }
